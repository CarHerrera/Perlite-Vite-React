filters:
  and:
    - "!Status.isEmpty()"
views:
  - type: cards
    name: View
    order:
      - file.name
      - Map
      - Status
      - Link
      - image
      - cover
    sort:
      - property: Status
        direction: DESC
    columnSize:
      file.name: 193
      note.Map: 435
    image: note.image
    imageFit: ""
